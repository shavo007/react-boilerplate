    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
    "node-sass": "4.11.0",
    "preload-webpack-plugin": "3.0.0-beta.3",
    "prettier": "1.15.3",
    "prettier-eslint": "8.8.2",
    "react-test-renderer": "16.7.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.28.3",
    "webpack-bundle-analyzer": "3.0.3",
